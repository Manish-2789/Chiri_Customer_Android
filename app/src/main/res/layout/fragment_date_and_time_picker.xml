<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/edit_phone_no_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#5ED6D4D4"
    tools:context=".DateAndTimePicker"
    android:clickable="true">

    <RelativeLayout
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_45sdp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true">

        <ImageView
            android:id="@+id/iv_back"
            android:layout_width="@dimen/_58sdp"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:paddingStart="@dimen/_20sdp"
            android:paddingEnd="@dimen/_20sdp"
            android:src="@drawable/ic_back_arrow"
            android:tint="@color/newColorBlack" />



    </RelativeLayout>

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/toolbar"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:paddingStart="15dp"
            android:paddingEnd="15dp">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="62dp"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:includeFontPadding="false"
                    android:text="Step 5 of 5"
                    android:textColor="@color/dark_gray"
                    android:textSize="12dp"
                    android:textStyle="normal" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:gravity="center_vertical"
                    android:includeFontPadding="false"
                    android:lineSpacingExtra="4dp"
                    android:text="Delivery Time"
                    android:textColor="@color/dark"
                    android:textSize="20dp"
                    android:textStyle="bold" />


            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:orientation="horizontal"
                android:weightSum="10">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@drawable/d_bottom_et_gray_line"
                    android:orientation="horizontal">

                    <androidx.cardview.widget.CardView
                        android:layout_width="120dp"
                        android:layout_height="70dp"
                        app:cardCornerRadius="5dp">

                        <LinearLayout
                            android:id="@+id/express"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:padding="5dp">

                                <TextView
                                    android:id="@+id/text"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Express"
                                    android:padding="15dp"
                                    android:textColor="@color/dark"
                                    android:textSize="20dp" />

                            </LinearLayout>



                        </LinearLayout>
                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="120dp"
                        android:layout_marginLeft="10dp"
                        android:layout_height="70dp"
                        app:cardCornerRadius="5dp">

                        <LinearLayout
                            android:id="@+id/line"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:padding="5dp">

                                <TextView
                                    android:id="@+id/text1"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="     Line"
                                    android:padding="15dp"
                                    android:textColor="@color/dark"
                                    android:textSize="20dp" />

                            </LinearLayout>



                        </LinearLayout>
                    </androidx.cardview.widget.CardView>
                    <androidx.cardview.widget.CardView
                        android:layout_width="120dp"
                        android:layout_marginLeft="10dp"
                        android:layout_height="70dp"
                        app:cardCornerRadius="5dp">

                        <LinearLayout
                            android:id="@+id/custom"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:padding="5dp">

                                <TextView
                                    android:id="@+id/text2"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Custom"
                                    android:padding="15dp"
                                    android:textColor="@color/dark"
                                    android:textSize="20dp" />

                            </LinearLayout>



                        </LinearLayout>
                    </androidx.cardview.widget.CardView>


                </LinearLayout>


            </LinearLayout>

            <RelativeLayout
                android:visibility="gone"
                android:id="@+id/tab_pickup_date_time"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_35sdp">


                <ImageView
                    android:scaleType="fitStart"
                    android:layout_width="@dimen/_18sdp"
                    android:layout_height="@dimen/_18sdp"
                    android:id="@+id/iv_calendar"
                    android:layout_marginStart="@dimen/_20sdp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentStart="true"
                    android:tint="@color/newColorLightHint"
                    android:src="@drawable/ic_calendar"
                    />

                <RelativeLayout
                    android:layout_marginStart="@dimen/_15sdp"
                    android:layout_marginEnd="@dimen/_20sdp"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_toEndOf="@+id/iv_calendar"
                    >

                    <LinearLayout
                        android:gravity="center_vertical"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <LinearLayout
                                android:layout_toStartOf="@+id/calandar_help_right"
                                android:layout_centerVertical="true"
                                android:weightSum="2"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/tv_select_pickup_datetime"
                                    android:layout_width="0dp"
                                    android:layout_weight="2"
                                    android:textAlignment="viewStart"
                                    android:layout_height="wrap_content"
                                    android:includeFontPadding="false"
                                    android:text="@string/select_pickup_date_time"
                                    android:textColor="@color/newColorDarkHint"
                                    android:textSize="@dimen/_10ssp"
                                    android:textStyle="normal"
                                    android:maxLines="2"
                                    android:fontFamily="@font/clan_ot_narrowmedium"
                                    android:gravity="center_vertical"
                                    tools:ignore="RtlCompat" />


                            </LinearLayout>

                            <ImageView
                                android:scaleType="fitEnd"
                                android:id="@+id/calandar_help_right"
                                android:layout_alignParentEnd="true"
                                android:layout_centerVertical="true"
                                android:layout_width="@dimen/_18sdp"
                                android:layout_height="@dimen/_18sdp"
                                android:src="@drawable/ic_help_alert"/>


                        </RelativeLayout>

                        <RelativeLayout
                            android:visibility="gone"
                            android:id="@+id/tab_title_pickup_datetime"
                            android:layout_marginTop="@dimen/_2sdp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <LinearLayout
                                android:layout_toStartOf="@+id/calandar_arrow_right"
                                android:weightSum="2"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/tv_pickup_date"
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="wrap_content"
                                    android:includeFontPadding="false"
                                    android:text=""
                                    android:textAlignment="viewStart"
                                    android:textColor="@color/newColorDarkHint"
                                    android:textSize="@dimen/_10ssp"
                                    android:textStyle="normal"
                                    android:maxLines="2"
                                    android:fontFamily="@font/clan_ot_narrowmedium"
                                    android:gravity="center_vertical"
                                    tools:ignore="RtlCompat" />
                                <TextView
                                    android:id="@+id/tv_pickup_time"
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="wrap_content"
                                    android:includeFontPadding="false"
                                    android:text=""
                                    android:textColor="@color/newColorDarkHint"
                                    android:textSize="@dimen/_10ssp"
                                    android:textStyle="normal"
                                    android:maxLines="2"
                                    android:textAlignment="viewEnd"
                                    android:fontFamily="@font/clan_ot_narrowmedium"
                                    android:gravity="center_vertical"
                                    tools:ignore="RtlCompat" />

                            </LinearLayout>

                            <ImageView
                                android:scaleType="fitEnd"
                                android:id="@+id/calandar_arrow_right"
                                android:layout_width="@dimen/_18sdp"
                                android:layout_height="@dimen/_18sdp"
                                android:layout_alignParentEnd="true"
                                android:src="@drawable/ic_arrow_right" />


                        </RelativeLayout>



                    </LinearLayout>



                </RelativeLayout>

            </RelativeLayout>




        </LinearLayout>



    </androidx.core.widget.NestedScrollView>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:orientation="vertical"
        android:paddingStart="15dp"
        android:paddingEnd="15dp">

        <LinearLayout
            android:id="@+id/btn_checkout"
            android:layout_width="match_parent"
            android:layout_height="38dp"
            android:layout_marginTop="25dp"
            android:layout_marginBottom="15dp"
            android:background="@drawable/ractengle_less_round_solid_primary"
            android:gravity="center"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:includeFontPadding="false"
                android:text="Checkout"
                android:textAllCaps="false"
                android:textColor="@color/newColorWhite"
                android:textSize="11dp"
                android:textStyle="normal" />


        </LinearLayout>
    </LinearLayout>


</RelativeLayout>
