<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/main_rl_id"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/newColorWhite">

            <RelativeLayout
                android:id="@+id/relative1"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/_50sdp"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_10sdp"
                android:layout_marginBottom="@dimen/_5sdp"
                android:background="@drawable/relative_search_bg">

                <View
                    android:id="@+id/view_id"
                    android:layout_width="@dimen/_3sdp"
                    android:layout_height="match_parent"
                    android:background="@color/newColorDarkHint"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/iv_vehicle"
                    android:layout_width="@dimen/_32sdp"
                    android:layout_height="@dimen/_32sdp"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/_15sdp"
                    android:contentDescription="@string/app_name"
                    android:src="@drawable/twowheeler" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/_5sdp"
                    android:layout_toEndOf="@+id/iv_vehicle"
                    android:gravity="center"
                    tools:ignore="RtlCompat">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:weightSum="1">

                        <LinearLayout
                            android:id="@+id/rl_id"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight=".7"
                            android:gravity="center_vertical|start"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/tv_vehicle_name"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/gilroy_bold"
                                android:gravity="center_vertical|start"
                                android:includeFontPadding="false"
                                android:singleLine="true"
                                android:textAlignment="viewStart"
                                android:textAllCaps="false"
                                android:textColor="@color/newColorDarkHint"
                                android:textSize="@dimen/_13ssp"
                                android:textStyle="normal"
                                tools:text="2 Wheeler" />

                            <TextView
                                android:id="@+id/tv_vehical_info"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_4sdp"
                                android:fontFamily="@font/gilroy_semibold"
                                android:gravity="center_vertical"
                                android:includeFontPadding="false"
                                android:singleLine="true"
                                android:textAllCaps="false"
                                android:textColor="@color/newColorLightHint"
                                android:textSize="@dimen/_10ssp"
                                android:textStyle="normal"
                                tools:text="Weight 500kg" />

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/rl_id2"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight=".3"
                            android:gravity="center_vertical|start"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:orientation="horizontal">

                                <ImageView
                                    android:layout_width="@dimen/_12sdp"
                                    android:layout_height="@dimen/_12sdp"
                                    android:src="@drawable/ic_ngn"
                                    android:tint="@color/newColorDarkHint" />

                                <TextView
                                    android:id="@+id/tv_vehicle_price"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/gilroy_bold"
                                    android:gravity="center_vertical|start"
                                    android:includeFontPadding="false"
                                    android:singleLine="true"
                                    android:textAlignment="viewStart"
                                    android:textAllCaps="false"
                                    android:textColor="@color/newColorDarkHint"
                                    android:textSize="@dimen/_14ssp"
                                    android:textStyle="normal"
                                    tools:text="15" />


                            </LinearLayout>

                            <TextView
                                android:id="@+id/tv_vehical_time"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_4sdp"
                                android:fontFamily="@font/gilroy_light"
                                android:gravity="center_vertical"
                                android:includeFontPadding="false"
                                android:singleLine="true"
                                android:textAllCaps="false"
                                android:textColor="@color/newColorLightHint"
                                android:textSize="@dimen/_10ssp"
                                android:textStyle="normal"
                                android:visibility="gone"
                                tools:text="6:38 PM" />

                        </LinearLayout>

                    </LinearLayout>

                </RelativeLayout>

            </RelativeLayout>

        </RelativeLayout>

    </FrameLayout>

</layout>

